<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
    <class name="Ogd.Virarium.Domain.Models.Connection, Ogd.Virarium.Domain"
           table="Connections">
        <id name="Id">
            <column name="Id"
                    sql-type="int"
                    not-null="true" />
            <generator class="identity" />
        </id>
        <one-to-one name="NICA"
                    column="NICAId" />
        <one-to-one name="NICB"
                    column="NICBId" />
    </class>
</hibernate-mapping>