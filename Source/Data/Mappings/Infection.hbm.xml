<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
    <class name="Ogd.Virarium.Domain.Models.Infection, Ogd.Virarium.Domain"
           table="Infections">
        <id name="Id">
            <column name="Id"
                    sql-type="int"
                    not-null="true" />
            <generator class="identity" />
        </id>
        <many-to-one name="Machine"
                     column="MachineId"/>
        <many-to-one name="Virus"
                     column="VirusId"/>
        <property name="Start" />
        <property name="[End]" />
    </class>
</hibernate-mapping>